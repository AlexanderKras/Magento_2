<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction" component="Magento_Ui/js/grid/tree-massactions">
            <action name="ship_with_domiciless">
                <settings>
                    <url path="laposte_colissimo/Orders/ChangeShipmentDomiciless"/>
                    <type>order_shipment_domiciless</type>
                    <label translate="true">Ship with: Colissimo Domicile without signature</label>
                </settings>
            </action>
            <action name="ship_with_domicileas">
                <settings>
                    <url path="laposte_colissimo/Orders/ChangeShipmentDomicileas"/>
                    <type>order_shipment_domicileas</type>
                    <label translate="true">Ship with: Colissimo Domicile with signature</label>
                </settings>
            </action>
            <action name="ship_with_domicileasddp">
                <settings>
                    <url path="laposte_colissimo/Orders/ChangeShipmentDomicileasddp"/>
                    <type>order_shipment_domicileasddp</type>
                    <label translate="true">Ship with: Colissimo Domicile with signature - DDP Option</label>
                </settings>
            </action>
        </massaction>
    </listingToolbar>
</listing>
